@model geoluxe_defect_logging.Models.SlabIDPartialModels

<body>
    @using (Html.BeginForm("SlabDetail", "QC_DefectDataInput", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
        @*@Html.AntiForgeryToken()*@
        <form class="form-horizontal">
            <div class="container">
                <div class="col-md-12">
                    <div class="form-group row">

                        <div class="col-md-6">
                            @Html.LabelFor(m => m.SlabID, null, new { @class = "control-label col-md-3" })
                            @Html.TextBoxFor(m => m.SlabID, null, new { @class = "form-control col-md-3", @type = "text" })
                            <button type="submit" class="btn btn-default" name="submit">
                                <span class="glyphicon glyphicon-play"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    }
</body>